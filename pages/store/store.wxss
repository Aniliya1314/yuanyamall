.store-head{
  display: flex;
height:130px;
  width: 100%;
  background-image: url(data:image/jpeg;base64,);
}
.avatar{
   margin:65rpx 10px;
height:80rpx;
width:80rpx;
border-radius:25px;
}
.store-name{
  display: block;
  margin-top: 25px;
}
.store-location{
width:13px;
height:13px;
padding-right:8rpx;
padding-top:12rpx;

}
.store-evaluation{
  display: flex;
  color:#fff;
}
.store-collection{
width:70px;
margin-top:25px;
height:25px;
font-size:10px;
text-align:center;
background-color:#f0145a;
color:#fff;
border-radius:50px;
}
.live{
  border-top: 5px solid  rgb(245, 242, 242);
  border-bottom: 5px solid rgb(245, 242, 242);
}
.live-top{
  font-size:15px;
  float:left;
  margin:4px 5px 5px 20px;
}
.live-down{
  margin-top: 29px;
}
.live-img{
   height: 130px;
   width: 130px;
}
.living{
  background:#f0145a;
width:70px;
height:16px;
border-radius:10px;
display:flex;
margin-top: 20px;
}

.tab-content1
{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tab-image
{
  width: 50%;
  margin-top: 130rpx;
  margin-bottom: 50rpx;
}
.swiper-tab{
    width: 100%;
    border-bottom: 2rpx solid #ccc;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    flex-flow: row;
    justify-content: space-between;
}
.swiper-tab-item{
    width: 30%;
    color:#434343;
    font-size: 14px;
}
.active{
    color:#F65959;
    border-bottom: 4rpx solid #F65959;
}
swiper{
  text-align: center;
  height:100vh;
}
.store-index{
  display: grid;
}

